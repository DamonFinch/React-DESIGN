{
  "name": "pcln-icons",
  "version": "5.6.0",
  "description": "Priceline React icons based on Material Design Icons",
  "main": "./dist/pcln-icons.cjs",
  "module": "./dist/pcln-icons.js",
  "exports": {
    ".": {
      "import": "./dist/pcln-icons.js",
      "require": "./dist/pcln-icons.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist components",
    "build": "run-s clean build:svg build:index build:copy vite:build",
    "build:svg": "svgr svg -d components",
    "build:index": "node ./create-index.js",
    "build:copy": "cp src/Icon.jsx src/Svg.js src/index.js components",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --silent --coverage",
    "test:ci": "jest --silent --coverage --runInBand",
    "test:update": "jest --silent --updateSnapshot",
    "test:snapshots": "jest --silent --runInBand --updateSnapshot",
    "vite:build": "vite build"
  },
  "files": [
    "dist/"
  ],
  "author": "Priceline",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@styled-system/theme-get": "^5.1.2",
    "lodash.upperfirst": "^4.3.1",
    "prop-types": "^15.8.1",
    "styled-system": "^4.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@priceline/babel-preset": "workspace:*",
    "@priceline/eslint-config": "workspace:*",
    "@rushstack/eslint-patch": "^1.1.0",
    "@svgr/cli": "^5.5.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "jest": "^26.6.3",
    "jest-standard-reporter": "^2.0.0",
    "jest-styled-components": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "pcln-design-system": "workspace:*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.11",
    "vite": "^4.3.9",
    "vite-bundle-visualizer": "^0.8.0",
    "vite-plugin-svgr": "^3.2.0"
  },
  "peerDependencies": {
    "pcln-design-system": "^5.7.0",
    "react": ">=16.10.0",
    "react-dom": ">=16.10.0",
    "styled-components": ">=5 <5.3.4 || >5.3.7 <6"
  }
}
