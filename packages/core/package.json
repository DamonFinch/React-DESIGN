{
  "name": "pcln-design-system",
  "version": "5.12.0",
  "description": "Priceline Design System",
  "type": "module",
  "main": "./dist/pcln-design-system.cjs",
  "module": "./dist/pcln-design-system.js",
  "exports": {
    ".": {
      "import": "./dist/pcln-design-system.js",
      "require": "./dist/pcln-design-system.cjs"
    }
  },
  "types": "types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/priceline/design-system.git"
  },
  "bugs": {
    "url": "https://github.com/priceline/design-system/issues"
  },
  "homepage": "https://github.com/priceline/design-system#readme",
  "engines": {
    "node": ">=14",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "vite:dev": "vite",
    "build": "vite build",
    "build:visualize": "vite-bundle-visualizer",
    "build:visualize:sunburst": "vite-bundle-visualizer --template sunburst",
    "build:visualize:network": "vite-bundle-visualizer --template network",
    "build:visualize:data": "vite-bundle-visualizer --template raw-data --output './vite-bundle-visualizer/stats.json'",
    "build:visualize:list": "vite-bundle-visualizer --template list --output './vite-bundle-visualizer/stats.yaml'",
    "build:watch": "vite build --watch",
    "vite:lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "vite:preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:snapshots": "vitest --update",
    "coverage": "vitest run --coverage"
  },
  "author": "Priceline",
